#komodo_button{
	background-image: url(/assets/km_button.png);
	display: table-cell !important;
	display:inline-table !important; 
	display:-moz-inline-box !important;
	display:inline-block !important;	
	letter-spacing:-1000em;
	text-indent:-1000em;
}
cite{
	font-style: italic;
	float: right;
}
.site_design,
.reminder_mac,
.reminder_win{
	float: left;
	margin: 0 1em 1em 0;
}

p,h3{
	clear: both;
}
/* @group Videos */

#inspirational_videos{
	margin: 15px 0;
	padding: 0;
}
#inspirational_videos li{
	width: 250px;
	height: 208px;
	overflow: hidden;
	float: left;
	margin: 0 6px;
}
* html #inspirational_videos li{
	display: inline;
}
#inspirational_videos object,
#inspirational_videos embed{
	width: 250px;
	height: 208px;
}

/* @end */